<?xml version="1.0" encoding="UTF-8"?>
<schemalist gettext-domain="currencyconverter">
  <schema id="@app-id@" path="/io/github/idevecore/CurrencyConverter/">
    <key name="width" type="i">
			<default>420</default>
		</key>
		<key name="height" type="i">
			<default>600</default>
		</key>
		<key name="is-maximized" type="b">
			<default>false</default>
		</key>
		<key name="is-fullscreen" type="b">
			<default>false</default>
		</key>
    <key type="s" name="color-scheme">
      <choices>
        <choice value="auto"/>
        <choice value="light"/>
        <choice value="dark"/>
      </choices>
      <default>"auto"</default>
    </key>
    <key type="s" name="src-currency">
        <default>'USD'</default>
    </key>
    <key type="s" name="dest-currency">
        <default>'EUR'</default>
    </key>
  </schema>
</schemalist>
