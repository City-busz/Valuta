{
    "app-id" : "io.github.idevecore.Valuta.Devel",
    "runtime" : "org.gnome.Platform",
    "runtime-version" : "45",
    "sdk" : "org.gnome.Sdk",
    "command" : "valuta",
    "finish-args" : [
        "--share=network",
        "--share=ipc",
        "--device=dri",
        "--socket=fallback-x11",
        "--socket=wayland",
        "--own-name=io.github.idevecore.Valuta.Devel.SearchProvider"
    ],
    "cleanup" : [
        "*blueprint*"
    ],
    "modules" : [
        "pypi-dependencies.json",
        {
            "name" : "Valuta",
            "builddir" : true,
            "buildsystem" : "meson",
            "config-opts" : [
                "--prefix=/app",
                "--buildtype=debugoptimized",
                "-Dprofile=development"
            ],
            "sources" : [
                {
                    "type" : "dir",
                    "url" : "./"
                }
            ]
        }
    ]
}
